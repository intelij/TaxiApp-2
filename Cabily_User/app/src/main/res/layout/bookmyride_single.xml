<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <LinearLayout
        android:id="@+id/bookmyride_single_car_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#00000000"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#00000000"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <View
                android:id="@+id/vvv"
                android:layout_width="match_parent"
                android:layout_height="5dp"


                />
            <com.mylibrary.widgets.CustomTextView
                android:id="@+id/bookmyride_single_carname"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:singleLine="true"
                android:text="5 min"
                android:textColor="#4e4e4e"
                android:textSize="11sp"
                 />


            <RelativeLayout
                android:id="@+id/car_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/bookmyride_single_carname">

                <View
                    android:id="@+id/book_my_ride_single_left_view"
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="-5dp"
                    android:layout_toLeftOf="@+id/book_my_ride_single_printer_icon_layout"
                    android:background="@drawable/line_side" />

                <RelativeLayout
                    android:id="@+id/book_my_ride_single_printer_icon_layout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true">
                    <com.mylibrary.widgets.CircularImageView
                        android:id="@+id/bookmyride_single_no_printer_image"
                        android:layout_width="45dp"
                        android:layout_height="45dp"


                        android:background="@drawable/category_bg"
                        android:visibility="visible" />

                    <com.mylibrary.widgets.CircularImageView
                        android:id="@+id/bookmyride_single_car_image"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_centerInParent="true"
                         />


                </RelativeLayout>

                <View
                    android:id="@+id/book_my_ride_single_right_view"
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:layout_marginLeft="-5dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/book_my_ride_single_printer_icon_layout"
                    android:background="@drawable/line_side" />
            </RelativeLayout>


          <!--  <ImageView
                android:id="@+id/bookmyride_single_car_image"
                android:layout_width="33dp"
                android:layout_height="33dp"
                android:layout_below="@+id/bookmyride_single_time"
                android:layout_weight="1"
                android:src="@drawable/car_icon" />-->

            <com.mylibrary.widgets.CustomTextView
                android:id="@+id/bookmyride_single_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/car_layout"
                android:layout_centerHorizontal="true"
                android:layout_weight="1"
                android:gravity="center"
                android:text="ghjghjhgjghj"
                android:textColor="#b3b3b3"
                android:textSize="10sp" />
            <View
                android:id="@+id/vv"
                android:layout_width="match_parent"
                android:layout_height="5dp"

                android:layout_below="@+id/bookmyride_single_time"
                 />


        </LinearLayout>


    </LinearLayout>

</LinearLayout>